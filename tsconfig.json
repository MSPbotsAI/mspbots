{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "target": "ESNext",
        "baseUrl": ".",
        "types": ["node"],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "paths": {
            "openclaw/plugin-sdk": [
                "./src/sdk-mocks.ts"
            ],
            "openclaw/plugin-sdk/*": [
                "./src/sdk-mocks.ts"
            ]
        }
    }
}